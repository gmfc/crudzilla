<h1>Lista de Locais</h1>
<a btn href="/crud/c/{{entityName}}">Cadastrar Local</a>
<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Endereço</th>
      <th>Número</th>
      <th></th>
      </tr>
  </thead>
  <tbody>
    {{#each list}}
    <tr>
      <td>{{this.name.[0]}}</td>
      <td>{{this.addres}}</td>
      <td>{{this.number}}</td>
      <td><a btn href="/crud/r/{{../entityName}}/{{this._id}}">-></a></td>
    </tr>
    {{/each}}
  </tbody>
</table>
